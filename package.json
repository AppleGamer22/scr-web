	"scripts": {
		"start": "ng serve client --poll=2000 & ng serve server",
		"start:docker": "ng serve client --poll=2000 --host=0.0.0.0 & ng serve server",
		"start:prod": "node dist/apps/server/main.js",
		"start:client": "ng serve client --poll=2000",
		"start:server": "ng serve server",
		"build": "ng build client --prod && ng build server",
		"build:client": "ng build client --prod",
		"build:server": "ng build server",
		"test": "ng test client && ng test server",
		"test:client": "ng test client",
		"test:server": "ng test server",
		"postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js"
